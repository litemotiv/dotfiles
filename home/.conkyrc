background no
cpu_avg_samples 2
net_avg_samples 2
no_buffers yes
out_to_console yes
out_to_stderr no
out_to_x no
update_interval 5.0
uppercase no
use_spacer none
mpd_host yuki
mpd_port 6600

TEXT
${if_mpd_playing}$mpd_artist - $mpd_title $mpd_elapsed/$mpd_length (${mpd_bitrate}k) |${endif} $loadavg - ${freq_g}Ghz ${platform coretemp.0 temp 1}°C/${platform coretemp.1 temp 1}°C | $battery | ${time %a %d/%m/%Y %T}
